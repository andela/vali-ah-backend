{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "db:migrate": "npm run sequelize-cli:es6 db:migrate",
    "db:rollback": "npm run db:undo && npm run db:migrate && npm run db:seed",
    "db:seed": "npm run sequelize-cli:es6 db:seed:all",
    "db:undo": "npm run sequelize-cli:es6 db:migrate:undo:all",
    "dev": "nodemon --exec babel-node src/index.js",
    "generate:doc": "npm run build && swagger-jsdoc -d dist/swagger.js  -o ./docs/swagger.json",
    "lint": "eslint \"**/*.js\" --fix",
    "pretest": "npm run db:undo && npm run db:migrate && npm run db:seed",
    "posttest": "npm run db:undo",
    "sequelize-cli:es6": "babel-node ./node_modules/.bin/sequelize $*",
    "start": "node dist/index.js",
    "start:dev": "cross-env DEBUG=dev nodemon --exec babel-node src/index.js",
    "start:debug": "cross-env DEBUG=dev nodemon --inspect --exec babel-node src/index.js",
    "sync-env": "sync-dotenv",
    "test": "mocha --require @babel/register --require regenerator-runtime/runtime tests/test.js --exit",
    "test:coverage": "nyc npm run test"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@sendgrid/mail": "^6.4.0",
    "async": "^3.1.0",
    "axios": "^0.19.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.14.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.17.1",
    "express-session": "^1.15.6",
    "express-validator": "5.3.1",
    "google-auth-library": "^5.1.0",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^2.2.1",
    "node-cron": "^2.0.3",
    "passport": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "random-bool": "^1.0.2",
    "request": "^2.87.0",
    "sequelize": "^5.10.2",
    "sinon-test": "^2.4.0",
    "socket.io": "^2.2.0",
    "swagger-ui-express": "^4.0.7",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.5",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "faker": "^4.1.0",
    "husky": "^3.0.1",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sequelize-cli": "^5.5.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "swagger-jsdoc": "^3.3.0",
    "sync-dotenv": "^2.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run sync-env && npm run lint && npm run test:coverage && npm run generate:doc"
    }
  }
}
